{
    "name": "endeavors/fhir-resource-generator",
    "type": "library",
    "description": "Generate fhir resources into php classes",
    "license": "MIT",
    "require": {
        "illuminate/console": "5.5.* || 5.6.* || 5.7.* || 5.8.*",
        "illuminate/support": "5.5.* || 5.6.* || 5.7.* || 5.8.*",
        "illuminate/filesystem": "5.5.* || 5.6.* || 5.7.* || 5.8.*",
        "dcarbone/php-fhir": "dev-master",
        "chumper/zipper": "1.0.x"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "symfony/var-dumper": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Endeavors\\Fhir\\": "src/",
            "Endeavors\\HL7\\Fhir\\": "output/Endeavors/HL7/Fhir"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endeavors\\Fhir\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Endeavors\\Fhir\\Providers\\FhirCommandServiceProvider"
            ]
        }
    }
}
